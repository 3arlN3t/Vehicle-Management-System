@model Transportation.Transportation_ViewAll

@{
    ViewBag.Title = "GetByDate";
}

<h2>GetByDate</h2>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-group">
        @Html.LabelFor(model => model.CheckOutTime, "CheckOut Time", htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.TextBoxFor(model => model.CheckOutTime, new { @type = "date" })
            @Html.ValidationMessageFor(model => model, "", new { @class = "text-danger" })
        </div>
    </div>
    <div class="form-group">
        @Html.LabelFor(model => model.CheckInTime, "CheckIn Time", htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.TextBoxFor(model => model.CheckInTime, new { @type = "date" })
            @Html.ValidationMessageFor(model => model, "", new { @class = "text-danger" })
        </div>
    </div>



<div class="form-group">
    <div class="col-md-offset-2 col-md-10">
        <input type="submit" value="Search" class="btn btn-default" />
    </div>
</div>
}