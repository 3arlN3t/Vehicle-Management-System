@{
    ViewBag.Title = "Transportation|SignIn/Out";
}

<!-- Custom CSS -->
<link rel="stylesheet" type="text/css" href="~/Content/MainStyles.css" />

<!-- Effects CSS-->
<link rel="stylesheet" type="text/css" href="~/Content/EffectsStyles.css" />


<div class="ButtonContainer row">

    <h1>Welcome!</h1>

    <br />
    <div class="col-md-6">
            <a onclick="location.href='@Url.Action("CheckOut", "Transportation")'"><span class="glyphicon glyphicon-chevron-up LargeIcon rotateRightSlow"></span></a>
